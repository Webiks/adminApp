{
  "name": "terra-admin-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://webiks.github.io/tb-admin-app/",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-scripts-ts": "2.16.0",
    "redux": "^4.0.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages --repo https://$GITHUB_TOKEN@github.com/webiks/tb-admin-app.git -d build",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "build:gh-pages": "npm run build && npm run copybuild && node scripts/gh-pages.js",
    "copybuild": "copyfiles build/**/* . --up 1",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "@types/react-redux": "^6.0.0",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3"
  }
}
